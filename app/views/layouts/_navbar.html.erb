<nav>
  <div class="container">
    <%= link_to "Posts", root_path %>
    <% if sign_in? %>
      <%= "User : #{@current_user.username}" %>
      <%= link_to "Sign out", session_path(current_user.id), method: :DELETE%>
    <% else %>
      <%= link_to "Log In", new_session_path %>
      <%= link_to "Sign Up", new_user_path %>
    <% end %>
  </div>
</nav>
